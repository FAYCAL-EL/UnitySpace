        <StackPanel>
            <Button Height="50" Margin="63,200,100,10">
                <StackPanel Orientation="Horizontal">
                    <Image HorizontalAlignment="Left" Source="Images/check-box.png" Width="18"></Image>
                    <Label Foreground="#fff" Margin="10,0,0,0" >RH Meetings I : new RH strategie</Label>
                    <Image HorizontalAlignment="Right" Source="Images/exit (1).png" Width="18" Margin="300,0,0,0"></Image>
                </StackPanel>
                <Button.Background>
                    <SolidColorBrush Color="Gray" Opacity="10"/>
                </Button.Background>
            </Button>
            <Button Height="50" Margin="63,0,100,10">
